{
    "name": "tecnobit/laravel-hubspot",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Cooper",
            "email": "therossjcooper@gmail.com"
        }
    ],
    "require": {
        "hubspot/api-client": "^4.0",
        "illuminate/support": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "Tecnobit\\LaravelHubSpot\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tecnobit\\LaravelHubSpot\\HubSpotServiceProvider"
            ],
            "aliases": {
                "HubSpot": "Tecnobit\\LaravelHubSpot\\Facades\\HubSpot"
            }
        }
    },
    "description": "Adds a Laravel specific wrapper for the Hubspot client package",
    "keywords": [
        "laravel",
        "hubspot",
        "api",
        "service"
    ],
    "require-dev": {
        "orchestra/testbench": ">=3.4",
        "friendsofphp/php-cs-fixer": "^2.15"
    }
}
